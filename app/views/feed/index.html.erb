<a href=<%=EnvironmentService.header_link%> id="up" class="hashtag-title">
  <h1><%=EnvironmentService.header_title%></h1>
</a>
<a id="hashtag-anchor"></a>

<div id="posts-list">
  <% @posts.each_with_index do |post, i| %>
    <%= render 'post', post: post, i: i %>
  <% end %>
</div>

<section class="load-older-posts">
	<div id="load-posts-btn"><a href="#load-posts-btn">Load Posts</a></div>
	<div id="loading"></div>
</section>
